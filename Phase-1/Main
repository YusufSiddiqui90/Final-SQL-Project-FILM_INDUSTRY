-- 16.music

CREATE TABLE music (
  track_id INT AUTO_INCREMENT PRIMARY KEY,
  movie_id INT,
  title VARCHAR(150),
  duration INT,
  singer VARCHAR(100),
  composer VARCHAR(100),
  lyricist VARCHAR(100),
  genre VARCHAR(50),
  release_date DATE,
  platform VARCHAR(100),
  FOREIGN KEY (movie_id) REFERENCES movies(movie_id)
);

