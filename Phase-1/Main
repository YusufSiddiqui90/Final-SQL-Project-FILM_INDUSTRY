-- 14.theaters

CREATE TABLE theaters (
  theater_id INT AUTO_INCREMENT PRIMARY KEY,
  name VARCHAR(150),
  location VARCHAR(100),
  city VARCHAR(50),
  state VARCHAR(50),
  country VARCHAR(50),
  total_screens INT,
  capacity INT,
  contact_number VARCHAR(20),
  owner_name VARCHAR(100)
);

