  resolution VARCHAR(20),
  views BIGINT,
  rating DECIMAL(3,1),
  FOREIGN KEY (movie_id) REFERENCES movies(movie_id),
  FOREIGN KEY (platform_id) REFERENCES streaming_platforms(platform_id)
);

