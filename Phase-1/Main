-- 11.trailers

CREATE TABLE trailers (
  trailer_id INT AUTO_INCREMENT PRIMARY KEY,
  movie_id INT,
  release_date DATE,
  duration INT,
  views BIGINT,
  likes BIGINT,
  dislikes BIGINT,
  published_on VARCHAR(50),
  youtube_link VARCHAR(200),
  language VARCHAR(50),
  FOREIGN KEY (movie_id) REFERENCES movies(movie_id)
);

