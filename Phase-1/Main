-- 15.showtimes

CREATE TABLE showtimes (
  showtime_id INT AUTO_INCREMENT PRIMARY KEY,
  movie_id INT,
  theater_id INT,
  screen_number INT,
  start_time TIME,
  end_time TIME,
  date DATE,
  format VARCHAR(20),
  language VARCHAR(20),
  is_premiere BOOLEAN,
  FOREIGN KEY (movie_id) REFERENCES movies(movie_id),
  FOREIGN KEY (theater_id) REFERENCES theaters(theater_id)
);

