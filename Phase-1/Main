Create database Films;
Use Films;


-- movies

CREATE TABLE movies (
  movie_id INT AUTO_INCREMENT PRIMARY KEY,
  title VARCHAR(200),
  genre VARCHAR(100),
  language VARCHAR(50),
  release_year YEAR,
  duration_minutes INT,
  budget DECIMAL(12,2),
  box_office DECIMAL(12,2),
  rating DECIMAL(3,1),
  director_id INT
);

INSERT INTO movies 
(title, genre, language, release_year, duration_minutes, budget, box_office, rating, director_id)
