-- 2.actors
CREATE TABLE actors (
  actor_id INT AUTO_INCREMENT PRIMARY KEY,
  full_name VARCHAR(150),
  gender VARCHAR(10),
  date_of_birth DATE,
  nationality VARCHAR(50),
  debut_year YEAR,
  awards_won INT,
  contact_email VARCHAR(150),
  phone_number VARCHAR(20),
  active_status BOOLEAN
);

